{
  "file": "test.proto",
  "generators": [
    {
      "name": "TS Models",
      "template": "templates/ts/model.ts.go.tpl",
      "output": "generated/ts/models/[model-].ts",
      "types": {
        "double": "number",
        "integer": "number",
        "string": "string",
        "date": "Date"
      },
      "helpers": {
        "\"now()\"": "new Date()"
      }
    },
    {
      "name": "TS Service",
      "template": "templates/ts/service.ts.go.tpl",
      "output": "generated/ts/services/[model-].service.ts",
      "types": {
        "double": "number",
        "integer": "number",
        "string": "string",
        "date": "Date"
      },
      "helpers": {
        "\"now()\"": "new Date()"
      }
    }
  ]
}
