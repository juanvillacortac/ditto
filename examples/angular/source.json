{
  "file": "schema.yml",
  "generators": [
    {
      "name": "TS Models",
      "template": "templates/model.ts.go.tpl",
      "output": "generated/models/[model-].ts",
      "types": {
        "int32": "number",
        "int64": "number",
        "double": "number",
        "boolean": "boolean",
        "string": "string",
        "date": "Date"
      },
      "helpers": {
        "\"now()\"": "new Date()"
      }
    },
    {
      "name": "TS Service",
      "template": "templates/service.ts.go.tpl",
      "output": "generated/services/[model-].service.ts",
      "types": {
        "int32": "number",
        "int64": "number",
        "double": "number",
        "boolean": "boolean",
        "string": "string",
        "date": "Date"
      },
      "helpers": {
        "\"now()\"": "new Date()"
      }
    }
  ]
}
